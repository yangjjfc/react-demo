// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "files.autoSave": "off",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    {
      "language": "jsx",
      "autoFix": true
    }
  ],
  "eslint.options": {
    "plugins": [
      "html",
      "jsx"
    ]
  },
  "eslint.autoFixOnSave": true,
  "eslint.enable": true
}